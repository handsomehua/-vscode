is = $(this); var e = event || window.event; var image = document.createElement('img'); image.className = 'altwei-big-img'; image.src = this.src; var width = image.width; var height = image.height; moveImage(width, height, e.pageX, e.pageY); $('body').append(image); $$this.on('mousemove', function(event) { var ee = event || window.event; moveImage(width, height, ee.pageX, ee.pageY); }); $$this.on('mouseleave', function() { $('.altwei-big-img').remove(); }); } </script>"
