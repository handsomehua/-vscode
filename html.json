se 'delete': layer.confirm('确定删除吗', { icon: 3 }, function(index) { layer.close(index); var checkStatus = table.checkStatus('list'); var ids = []; $(checkStatus.data).each(function(index, item) { ids.push(item.id); }); if (ids == '') { layer.msg('请选择数据', { time: 1000, icon: 2 }); return false; } post('{:url(\"Commons/layDelete\")}', tb, ids) }); break; } }); table.on('tool(list)', (obj) => { switch (obj.event) { case 'update': layer.alert('id：' + obj.data.id); break; case 'delete': layer.confirm('确定删除吗', { icon: 3 }, function(index) { post('{:url(\"Commons/layDelete\")}', tb, obj.data.id); }); break; } }); table.on('edit(list)', function(obj) { var value = obj.value , data = obj.data, field = obj.field; if (value) { $.post('{:url(\"Commons/layUpdate\")}', { 'id': data.id, 'field': field, 'value': value, 'table':tb}, function(res) { if (res.code > 0) { layer.msg(res.msg, { time: 1800, icon: 1 }, function() { tableIn.reload(); }); } else { layer.msg(res.msg, { time: 1800, icon: 2 }); } }) } }); form.on('select(selectDemo)', function(obj) { console.log(obj.elem.getAttribute('dataId')); console.log(obj.elem.getAttribute('name')); console.log(obj.value); $.ajax({ url: '{:url(\"admin/Commons/layUpdate\")}', data: {id: obj.elem.getAttribute('dataId'), field: obj.elem.getAttribute('name'), value: obj.value,table: tb }, type: 'post', async: false, success: function(res) { console.log(res); if (res.code > 0) { layer.msg(res.msg, { time: 1800, icon: 1 }, function() { tableIn.reload(); }); } else { layer.msg(res.msg, { time: 1800, icon: 2 }); } } }) });   form.on('switch(status)', function(data) {            var status; if (data.elem.checked) { status = 0; } else { status = 1; } alert(status); }) function post(url, table, id) { var loading = layer.load(1, { shade: [0.1, '#fff'] }); $.post(url, { id: id, 'table': table }, function(res) { layer.close(loading); if (res.code > 0) { layer.msg(res.msg, { time: 1000, icon: 1 }); tableIn.reload(); } else { layer.msg(res.msg, { time: 100